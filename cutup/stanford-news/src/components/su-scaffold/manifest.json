{
  "$schema": "http://localhost:3000/schemas/v1.json",
  "name": "scaffold",
  "namespace": "stanford-components",
  "displayName": "Scaffold",
  "description": "Component scaffold",
  "version": "0.0.1",
  "mainFunction": "main",
  "environment": [],
  "functions": [
    {
      "name": "main",
      "entry": "main.js",
      "input": {
        "type": "object",
        "title": "Component scaffold",
        "description": "",
        "required": [],
        "properties": {
          "content": {
            "type": "string",
            "title": "Content",
            "default": ""
          },
          "textColor": {
            "type": "object",
            "title": "Text color",
            "enum": [{"Black": "su-text-black"}, {"White": "su-text-white"}],
            "description": "Applied to container as class.",
            "default": ""
          },
          "bgColor": {
            "type": "object",
            "title": "Background Color",
            "enum": [{"Black": "su-bg-black"}, {"White": "su-bg-white"}, {"Blue": "su-bg-digital-blue-dark"}],
            "description": "Applied to container as class.",
            "default": ""
          }
        }
      },
      "output": {
        "responseType": "html",
        "staticFiles": []
      }
    }
  ],
  "previews": {
    "default": {
      "functionData": {
        "main": {
          "inputData": {
            "type": "file",
            "path": "data/sample.json"
          },
          "wrapper": {
            "path": "previews/wrapper.html"
          }
        }
      }
    }
  },
  "staticFiles": {
    "locationRoot": "static"
  }
}
