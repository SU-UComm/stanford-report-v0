{
  "name": "su-components",
  "version": "0.0.1",
  "description": "Stanford University components",
  "main": "index.js",
  "scripts": {
    "audit": "npm audit --production",
    "test": "jest --verbose --passWithNoTests",
    "test:coverage": "jest --coverage",
    "test:all": "npm-run-all test cypress:ci:dev ",
    "build": "webpack --config webpack/webpack.prod.js --env minify=false",
    "build-min": "webpack --config webpack/webpack.prod.js --env minify=true",
    "dev": "concurrently \"npm run serve:no-open\" \"npm run dxp\" \"npm run loco\"",
    "devServe": "concurrently \"npm run serve\" \"npm run loco\"",
    "dxp": "dxp-next cmp dev ./src/components",
    "serve": "webpack-dev-server --open --config webpack/webpack.dev.js",
    "build:serve": "npm run build && webpack-dev-server --open --config webpack/webpack.dev.js",
    "serve:no-open": "webpack-dev-server --config webpack/webpack.dev.js",
    "lint": "npm-run-all lint:js lint:css",
    "lint:js": "eslint -f codeframe \"./src/**/*.js\" \"./src/**/*.jsx\" --no-error-on-unmatched-pattern",
    "lint:css": "stylelint \"./src/**/*.css\" \"./src/**/*.scss\" \"!./src/**/*.min.css\" \"!./src/templates/**/*.scss\"",
    "format": "prettier --ignore-path .prettierignore --write \"./src/modules/**/*.+(js|jsx|json)\"",
    "cypress:install": "npm install cypress@10.0.3 --no-save",
    "cypress:install:save": "npm install -sD cypress@10.0.3",
    "cypress:verify": "cypress verify",
    "cypress:dev": "npx cypress open --config-file cypress.development.config.js",
    "cypress:prod": "npx cypress open --config-file cypress.production.config.js",
    "cypress:ci:dev": "cypress run --browser chrome --headless --config-file cypress.development.config.js",
    "cypress:ci": "cypress run --browser chrome --headless --config-file cypress.production.config.js",
    "loco": "loco loco.json",
    "storybook": "concurrently \"start-storybook -p 6006\" \"npm run serve:no-open\" \"npm run loco\"",
    "build-storybook": "build-storybook"
  },
  "moduleNameMapper": {},
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.18.6",
    "@cypress/webpack-preprocessor": "^5.10.0",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-controls": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack4": "^6.5.16",
    "@storybook/html": "^6.5.16",
    "@storybook/manager-webpack4": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "ansi-regex": "^6.0.1",
    "autoprefixer": "^10.4.0",
    "axe-core": "^4.3.5",
    "babel-loader": "^8.2.3",
    "chalk": "^4.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^8.0.1",
    "copy-webpack-plugin": "^9.1.0",
    "core-js": "^3.19.1",
    "css-loader": "^6.7.3",
    "cypress-axe": "^0.13.0",
    "cypress-dotenv": "^2.0.0",
    "dotenv": "^10.0.0",
    "dotenv-webpack": "^7.0.3",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-formatter-codeframe": "^7.32.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^25.2.4",
    "eslint-plugin-react": "^7.20.0",
    "eslint-webpack-plugin": "^3.1.1",
    "file-loader": "^6.2.0",
    "glob": "^7.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "import-glob-loader": "^1.1.0",
    "jest": "^27.3.1",
    "loader-utils": "^1.0.2",
    "loco-server": "^3.1.0",
    "mini-css-extract-plugin": "^2.4.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^8.3.0",
    "postcss-sass": "^0.5.0",
    "prettier": "^2.2.1",
    "sass": "^1.43.4",
    "sass-loader": "^12.3.0",
    "style-loader": "^3.3.2",
    "stylelint": "^14.1.0",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-config-standard-scss": "^2.0.1",
    "stylelint-config-tailwindcss": "^0.0.7",
    "stylelint-scss": "^4.0.0",
    "stylelint-webpack-plugin": "^3.1.0",
    "tailwindcss": "^3.3.1",
    "webpack": "^5.64.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.4",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@emotion/core": "^10.0.35",
    "@emotion/styled": "^10.0.27",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-app-polyfill": "^2.0.0",
    "react-dom": "^16.14.0",
    "react-router-dom": "^6.0.2"
  }
}
